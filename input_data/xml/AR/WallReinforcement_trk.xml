    <TextId>5001</TextId>
    <Text>Genel sekmesine erişin.</Text>
  </Item>

  <Item>
    <TextId>5003</TextId>
    <Text>Ana Donatı sekmesine erişin.</Text>
  </Item>

  <Item>
    <TextId>5004</TextId>
    <Text>Ek donatı sekmesine erişin.</Text>
  </Item>

  <Item>
    <TextId>5005</TextId>
    <Text>Etriyeler sekmesine erişin.</Text>
  </Item>

  <Item>
    <TextId>5007</TextId>
    <Text>Kesitler sekmesine erişin.</Text>
  </Item>

  <Item>
    <TextId>5008</TextId>
    <Text>Biçim sekmesine erişin.</Text>
  </Item>

  <Item>
    <TextId>10010</TextId>
    <Text>Aşağıdaki hatalardan dolayı donatı oluşturulamıyor: \n</Text>
  </Item>

